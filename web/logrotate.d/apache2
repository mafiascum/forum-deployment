/opt/bitnami/apache2/logs/access/raw/*-file.log /opt/bitnami/apache2/logs/error/*-file.log {
	daily
	missingok
	rotate 36500
	compress
	compresscmd /usr/bin/zip
	compressext .zip
	dateext
	dateformat .%Y%m%d
	delaycompress
	notifempty
	create 640 root adm
	sharedscripts
	postrotate
		/opt/bitnami/scripts/mafiascum/ms_reload_apache.sh
	endscript
}