/opt/bitnami/apache2/logs/access/*-file.log /opt/bitnami/apache2/logs/error/*-file.log {
	daily
	missingok
	rotate 36500
	compress
	compresscmd /usr/bin/zip
	compressext .zip
	dateext
	dateformat .%Y%m%d
	delaycompress
	notifempty
	create 640 root adm
	sharedscripts
	postrotate
		echo "Top of postrotate" >> /tmp/postrotate.txt 2>&1
		/usr/bin/whoami >> /tmp/postrotate.txt 2>&1
		/opt/bitnami/apache/bin/apachectl -k graceful >> /tmp/postrotate.txt 2>&1
		echo "Bottom of postrotate" >> /tmp/postrotate.txt 2>&1
	endscript
}